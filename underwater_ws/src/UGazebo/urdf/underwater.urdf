            </geometry>
        </collision>

        <visual>
            <origin rpy="0 0 0" xyz="0 0 -.0976"/>
            <geometry>
                <mesh filename="package://underwater_sim/models/meshes/robot/MainBody.dae" scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="white"/>
        </visual>
        <plugin name="thruster_plug" filename="libthruster_plug.so"/>
    </link> 

    <link name="center_link">
