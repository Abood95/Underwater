
<robot name="underwater" xmlns:xacro="http://www.ros.org/wiki/xacro">

    <!-- * * * Color Definitions * * * -->
    

    <material name="white">
        <color rgba="1 1 1 1"/>
    </material>

    <material name="green2">
        <color rgba="0.8 0.9 0.8 0.9"/>
    </material> 

    <material name="red">
        <color rgba="0.8 0 0 1"/>
    </material>

    <material name="gray">
        <color rgba="0.5 0.5 0.5 0.9"/>
    </material>

    <material name="transparent">
